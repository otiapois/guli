<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.fouo.coupon.mapper.HomeSubjectMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="homeSubjectResultMap" type="com.fouo.coupon.entity.HomeSubject">
        <id column="id" property="id"/>
        <result column="name" property="name"/>
        <result column="title" property="title"/>
        <result column="sub_title" property="subTitle"/>
        <result column="status" property="status"/>
        <result column="url" property="url"/>
        <result column="sort" property="sort"/>
        <result column="img" property="img"/>
    </resultMap>


    <select id="selectHomeSubjectPage" resultMap="homeSubjectResultMap">
        select * from sms_home_subject where is_deleted = 0
    </select>

</mapper>
