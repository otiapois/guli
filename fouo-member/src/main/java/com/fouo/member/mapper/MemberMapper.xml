<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.fouo.member.mapper.MemberMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="memberResultMap" type="com.fouo.member.entity.Member">
        <id column="id" property="id"/>
        <result column="level_id" property="levelId"/>
        <result column="username" property="username"/>
        <result column="password" property="password"/>
        <result column="nickname" property="nickname"/>
        <result column="mobile" property="mobile"/>
        <result column="email" property="email"/>
        <result column="header" property="header"/>
        <result column="gender" property="gender"/>
        <result column="birth" property="birth"/>
        <result column="city" property="city"/>
        <result column="job" property="job"/>
        <result column="sign" property="sign"/>
        <result column="source_type" property="sourceType"/>
        <result column="integration" property="integration"/>
        <result column="growth" property="growth"/>
        <result column="status" property="status"/>
        <result column="create_time" property="createTime"/>
    </resultMap>


    <select id="selectMemberPage" resultMap="memberResultMap">
        select * from ums_member where is_deleted = 0
    </select>

</mapper>
